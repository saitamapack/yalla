{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.0"
    },
    "api/clean.php": {
      "runtime": "vercel-php@0.7.0"
    },
    
    "api/*.php": {
      "runtime": "vercel-php@0.7.0",
      "maxDuration": 60
    }
  },
  "routes": [
    { "src": "/", "dest": "/api/index.php" },
    { "src": "/clean", "dest": "/api/clean.php" },
    
    { "src": "/:file([^/]+\\)", "dest": "/api/:file.php" },
    { "src": "/(.*)", "dest": "/api/index.php" }
  ]
}
